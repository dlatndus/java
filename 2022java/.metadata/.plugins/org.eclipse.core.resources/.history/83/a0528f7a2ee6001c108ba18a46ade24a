package java_test;

import java.io.BufferedWriter;
import java.io.File;
import java.io.FileNotFoundException;
import java.io.FileWriter;
import java.io.IOException;

public class make_sung_read2 {
	int length;
	int[] snum = new int[10];
	String[] sname = new String[10];
	double[] korean = new double[10];
	double[] math = new double[10];
	double[] english = new double[10];
	double[] total = new double[10];
	double[] avg = new double[10];
	String[] grade = new String[10];
	double[] st = new double[10];
	
	public void writing(String fname) throws IOException {
		FileWriter fw = null;
		BufferedWriter bw = null;
		try {
			fw = new FileWriter(new File(fname));
			bw = new BufferedWriter(fw);
			
			//total
			for(int i =0; i< length; i++) {
				
			}
			
			//avg
			for(int i =0; i< length; i++) {
				
			}
			
			//grade
			for(int i =0; i< length; i++) {
				
			}
			
			//st
			for(int i =0; i< length; i++) {
				
			}
			
			for (int j = 0; j < length; j++) {
				bw.write(snum[j] + "\t" + sname[j] + "\t" + korean[j] + "\t" + math[j] + "\t" + english[j] 
						+ "\t" + total[j] + "\t" + avg[j]+ "\t" + grade[j] + "\t" + st[j]);
				bw.newLine();
			}
			
			bw.close();
			fw.close();
		} catch (FileNotFoundException e) {
			System.out.println("잘못된 파일 이름을 입력했습니다");
		}
	}

	
		

	

}
